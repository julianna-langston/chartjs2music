{
  "name": "chartjs-plugin-chart2music",
  "version": "0.6.0",
  "type": "module",
  "description": "Chart.js plugin for Chart2Music. Turns chart.js charts into music so the blind can hear data.",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "exports": {
    "import": "./dist/plugin.mjs",
    "require": "./dist/plugin.cjs"
  },
  "types": "dist/plugin.d.ts",
  "files": [
    "dist/*"
  ],
  "keywords": [
    "a11y",
    "accessibility",
    "chart.js",
    "chart",
    "dataviz",
    "screen reader",
    "sonification"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/julianna-langston/chartjs2music"
  },
  "bugs": {
    "url": "https://github.com/julianna-langston/chartjs2music/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "rollup -c rollup.config.js --silent && yarn build-cjs",
    "build-cjs": "babel ./dist/plugin.mjs --out-file ./dist/plugin.cjs",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "depcheck": "depcheck",
    "clean": "rimraf dist coverage",
    "prepack": "npm run clean && npm run build"
  },
  "devDependencies": {
    "@babel/cli": "7.25.9",
    "@babel/core": "7.26.0",
    "@babel/plugin-transform-modules-commonjs": "7.25.9",
    "@babel/preset-env": "7.26.0",
    "@rollup/plugin-typescript": "12.1.1",
    "@sgratzl/chartjs-chart-boxplot": "4.4.3",
    "@types/jest": "29.5.14",
    "canvas": "2.11.2",
    "chartjs-chart-wordcloud": "4.4.3",
    "chartjs-plugin-a11y-legend": "0.2.0",
    "cross-env": "7.0.3",
    "depcheck": "1.4.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "rollup": "4.25.0",
    "rollup-plugin-copy": "3.5.0",
    "ts-jest": "29.2.5",
    "tslib": "2.8.1",
    "typescript": "5.6.3",
    "vite": "6.0.3"
  },
  "dependencies": {
    "chart.js": "4.4.6",
    "chart2music": "1.17.0"
  }
}

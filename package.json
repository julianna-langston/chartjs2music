{
  "name": "chartjs-plugin-chart2music",
  "version": "0.4.0",
  "type": "module",
  "description": "Chart.js plugin for Chart2Music. Turns chart.js charts into music so the blind can hear data.",
  "main": "dist/plugin.js",
  "module": "dist/plugin.mjs",
  "exports": {
    "import": "./dist/plugin.mjs",
    "require": "./dist/plugin.cjs"
  },
  "files": [
    "dist/*"
  ],
  "keywords": [
    "a11y",
    "accessibility",
    "chart.js",
    "chart",
    "dataviz",
    "screen reader",
    "sonification"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/julianna-langston/chartjs2music"
  },
  "bugs": {
    "url": "https://github.com/julianna-langston/chartjs2music/issues"
  },
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "rollup -c rollup.config.js --silent && yarn build-cjs",
    "build-cjs": "babel ./dist/plugin.mjs --out-file ./dist/plugin.cjs",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "depcheck": "depcheck",
    "clean": "rimraf dist coverage"
  },
  "devDependencies": {
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/plugin-transform-modules-commonjs": "7.24.1",
    "@babel/preset-env": "7.24.4",
    "@rollup/plugin-typescript": "11.1.6",
    "@sgratzl/chartjs-chart-boxplot": "4.3.1",
    "@types/jest": "29.5.12",
    "canvas": "2.11.2",
    "chartjs-chart-wordcloud": "4.4.0",
    "chartjs-plugin-a11y-legend": "0.1.1",
    "cross-env": "7.0.3",
    "depcheck": "1.4.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "rimraf": "5.0.5",
    "rollup": "4.14.2",
    "ts-jest": "29.1.2",
    "tslib": "2.6.2",
    "typescript": "5.4.5",
    "vite": "5.2.8"
  },
  "dependencies": {
    "chart.js": "4.4.2",
    "chart2music": "1.16.2"
  }
}
